<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name ="factoryMethod.customFactories"
             type ="System.Configuration.NameValueSectionHandler"/>
    <section name ="concreteProducts"
             type ="System.Configuration.NameValueSectionHandler"/>
  </configSections>
  
  <factoryMethod.customFactories>
    <add key="PracticalPatterns.FactoryMethod.Tests.Classic.IFactory,FactoryMethod.Tests"
         value="PracticalPatterns.FactoryMethod.Tests.Classic.FactoryA,FactoryMethod.Tests"/>
  </factoryMethod.customFactories>

  <concreteProducts>
    <add key ="DEFAULT"
         value ="PracticalPatterns.FactoryMethod.ProductA, FactoryMethod"/>
    <add key ="a"
         value ="PracticalPatterns.FactoryMethod.ProductA, FactoryMethod"/>
    <add key ="b"
         value ="PracticalPatterns.FactoryMethod.ProductB, FactoryMethod"/>
  </concreteProducts>
</configuration>